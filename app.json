{
  "expo": {
    "name": "GeoFence",
    "description": "This project is a demo for geo-fencing",
    "slug": "geofence",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "primaryColor": "#FFFFFF",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.justinjoyn.geofence",
      "isRemoteJSEnabled": false,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your device location to track live location"
      }
    },
    "androidStatusBar": {
      "backgroundColor": "#FFFFFF",
      "barStyle": "dark-content"
    },
    "android": {
      "package": "com.justinjoyn.geofence",
      "permissions": [
        "ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDg3A1U1xwvThCxdvxHxsZzH6XvhtuXh3U"
        }
      }
    }
  }
}
